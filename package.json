{
    "name": "@webmogilevtsev/nestjs-ddd-cli",
    "version": "1.0.20",
    "description": "Domain Driven Design (DDD) Structure Generator is a CLI tool for quickly creating well-organized application architectures based on DDD principles. It generates boilerplate code for essential building blocks, allowing developers to focus on solving complex business problems while reducing development time and enhancing software quality.",
    "author": "Mogilevtsev Dmitry <webmogilevtsev@ya.ru>",
    "license": "MIT",
    "main": "dist/main.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/mogilevtsevdmitry/nestjs-ddd-cli"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "nestjs",
        "ddd",
        "cli"
    ],
    "bin": {
        "generate-domain": "./bin/generate-domain"
    },
    "readme_en_md_content": "[English readme content goes here]",
    "readme_ru_md_content": "[Russian readme content goes here]",
    "scripts": {
        "postinstall": "npm run build && node postinstall.js",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "generate:domain": "node ./dist/main.js ddd-cli --",
        "g:domain": "node ./dist/main.js ddd-cli --"
    },
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.0.0",
        "@nestjs/cqrs": "^9.0.3",
        "@nestjs/platform-express": "^9.0.0",
        "chalk": "^5.2.0",
        "commander": "^10.0.1",
        "fs-extra": "^11.1.1",
        "nest-commander": "^3.7.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.2.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^11.0.1",
        "@types/jest": "29.5.0",
        "@types/node": "18.15.11",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "29.5.0",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "29.0.5",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.2.0",
        "typescript": "^4.7.4"
    },
    "peerDependencies": {
        "@nestjs/cqrs": "^9.0.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/src/"
        ]
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
